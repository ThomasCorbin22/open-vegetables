<div class='container-fluid'>
    <div class='row '>
        <div class='col-sm-10 '>
            {{> user_nav}}
            {{!-- <div class='row justify-content-center'>
                <img class='img-fluid' src="/pictures/Screenshot 2020-06-06 at 3.46.29 PM.png" alt="">
            </div> --}}
            <div class='container-fluid'>
                <div class='row'>
                    <div class='col-sm-12'>
                        {{#each blogs}}
                        <div class='card custom-height overflow-auto'>
                                <a href='/blog/blogNumber{{id}}'>
                                    <h2 class='card-header'> {{title}}</h2>
                                </a>
                            <div class='card-body'>
                                <h7 class='card-subtitle text-muted'>{{categories}}</h7>
                                <h7 class='card-subtitle text-muted'>{{date_created}} </h7>
                                <h7 class='card-subtitle text-muted'>{{date_modified}}</h7>
                                <p class='card-text'>{{body}}</p>
                                <img class='card-img custom-area-small' src='{{pictures}}'>
                            </div>
                        </div>
                 
                    {{/each}}
                    </div>
                </div>
            </div>
        </div>
        <div class='col-sm-2'>

        </div>
    </div>
</div>